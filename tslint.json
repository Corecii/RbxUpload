{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "double"],
      "object-literal-sort-keys": false,
      "no-console": false,
      "whitespace": [
          true,
          "check-branch",
          "check-decl",
          "check-module",
          "check-separator",
          "check-rest-spread",
          "check-type",
          "check-typecast",
          "check-type-operator",
          "check-preblock",
          "check-postbrace"
      ],
      "max-classes-per-file": false,
      "object-literal-shorthand": false,
      "max-line-length": false
    },
    "rulesDirectory": []
}